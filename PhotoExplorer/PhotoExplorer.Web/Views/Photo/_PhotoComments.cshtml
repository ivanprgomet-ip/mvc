@model PhotoExplorer.Web.Models.PhotoDetailsViewModel

@{
    foreach (var comment in Model.Comments)
    {
        <article class="breadcrumb">
            <small>
                @comment.User.Username commented:
            </small>
            <p>
                @comment.Comment
            </p>
            <small>
                @comment.DateCreated
            </small>
        </article>
    }
}
