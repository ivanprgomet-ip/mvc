@model PhotoExplorer.Web.Models.UserModel

<h2>@Model.Username Details</h2>

<article class="user">
    <div class="col-lg-2 col-md-2 col-xs-4">
        <i class="glyphicon glyphicon-user" style="font-size:36px;"></i>
        <div style="display:flex;flex-direction:column;">
            <small style="font-weight:800">Fullname: @Model.Fullname</small>
            <small>Date Registered: @Model.DateRegistered</small>
            <small>Username: @Model.Username</small>
            show user albums
        </div>
    </div>
</article>