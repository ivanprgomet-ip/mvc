@model PhotoExplorer.Web.Models.AlbumViewModel

@foreach (var photo in Model.Photos)
{
    <div class="col-lg-3 col-md-4 col-xs-6 thumb">
        <a class="thumbnail" href="@Url.Action("Details","Photo",new {id=photo.Id },null)">
            <img class="img-responsive" src="~/photos/@photo.FileName" alt="">
        </a>
    </div>
}